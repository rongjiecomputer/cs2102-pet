<%- include('header', {title: 'Previously Accepted Jobs'}); -%>
<main class="container">
    <h2>Your previously accepted jobs</h2>
    <% if (results.length == 0) { %>
    <div class="">No result found.</div>
    <% } else { %>
        <table class="table table-borderless table-hover">
            <thead class="thead-light" >
            <th>Pet Owner's Name</th>
            <th>Service Type</th>
            <th>Date Accepted</th>

            </thead>
            <tbody>
            <% for (const row of results) { %>
            <tr>
                <td><%= row.name %></td>
                <td><%= row.servicename %></td>
                <td><%= row.dateaccepted.toLocaleDateString() %></td>
            </tr>
            <% } %>
            </tbody>
        </table>

    <% } %>
</main>
<%- include('footer'); -%>
